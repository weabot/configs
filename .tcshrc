#VARIABLES
set prompt = "%B%{\033[32m%}%n%{\033[33m%}@%{\033[36m%}%m%b: %{\033[32m%}%B%~%b%# "
set autolist set color set colorcat
set EDITOR=vim
set VISUAL=vim

#ALIAS
alias ls ls --color='always'
alias ttympv mpv -vo drm --profile=pseudo-gui
alias cc clang
alias cpp clang++
alias cxx clang++

#KEYBINDS
bindkey -v
